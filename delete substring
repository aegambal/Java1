import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        String S= sc.nextLine();
        int a = sc.nextInt();
        int b = sc.nextInt();
        StringBuffer sb= new StringBuffer(S);
        sb.delete(a,b);
        System.out.print(sb);
    }
}
